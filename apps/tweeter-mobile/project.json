{
  "root": "apps/tweeter-mobile",
  "sourceRoot": "apps/tweeter-mobile/src",
  "projectType": "application",
  "targets": {
    "start": {
      "executor": "@nrwl/react-native:start",
      "options": {
        "port": 8081
      }
    },
    "serve": {
      "executor": "@nrwl/workspace:run-commands",
      "options": {
        "commands": [
          "nx run tweeter-mobile:tailwind-dev",
          "nx start tweeter-mobile"
        ]
      }
    },
    "tailwind-dev": {
      "executor": "@nrwl/workspace:run-commands",
      "options": {
        "cwd": "apps/tweeter-mobile",
        "command": "concurrently \"tailwindcss --input input.css --output tailwind.css --no-autoprefixer --watch\" \"tailwind-rn --watch\""
      }
    },
    "tailwind-build": {
      "executor": "@nrwl/workspace:run-commands",
      "options": {
        "cwd": "apps/tweeter-mobile",
        "command": "tailwindcss --input input.css --output tailwind.css --no-autoprefixer && tailwind-rn"
      }
    },
    "run-ios": {
      "executor": "@nrwl/react-native:run-ios",
      "options": {
        "simulator": "iPhone 13"
      }
    },
    "bundle-ios": {
      "executor": "@nrwl/react-native:bundle",
      "outputs": ["apps/tweeter-mobile/build"],
      "options": {
        "entryFile": "src/main.tsx",
        "platform": "ios",
        "bundleOutput": "dist/apps/tweeter-mobile/ios/main.jsbundle"
      }
    },
    "run-android": {
      "executor": "@nrwl/react-native:run-android",
      "options": {}
    },
    "build-android": {
      "executor": "@nrwl/react-native:build-android",
      "outputs": [
        "apps/tweeter-mobile/android/app/build/outputs/bundle",
        "apps/tweeter-mobile/android/app/build/outputs/apk"
      ],
      "options": {}
    },
    "bundle-android": {
      "executor": "@nrwl/react-native:bundle",
      "options": {
        "entryFile": "src/main.tsx",
        "platform": "android",
        "bundleOutput": "dist/apps/tweeter-mobile/android/main.jsbundle"
      }
    },
    "sync-deps": {
      "executor": "@nrwl/react-native:sync-deps",
      "options": {}
    },
    "ensure-symlink": {
      "executor": "@nrwl/react-native:ensure-symlink",
      "options": {}
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["apps/tweeter-mobile/**/*.{ts,tsx,js,jsx}"]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/apps/tweeter-mobile"],
      "options": {
        "jestConfig": "apps/tweeter-mobile/jest.config.ts",
        "passWithNoTests": true
      }
    }
  },
  "tags": []
}
